<h2>ADMIN DASH</h2>

<h3><%= @org[0][:name] %>'s Users</h3>

<ul>
  <% @users.each do |user| %>
    <%= content_tag_for :li, user do %>
      <%= user.name %>
    <% end %>
  <% end %>
</ul>
